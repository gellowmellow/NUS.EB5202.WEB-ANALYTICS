{
  "intents": [
    {
      "tag": "greeting",
      "patterns": [ "Hi", "How are you", "Is anyone there?", "Hello", "Good day" ],
      "responses": [ "Hello, thanks for visiting", "Good to see you again", "Hi there, how can I help?" ],
      "context_set": ""
    },

    {
      "tag": "goodbye",
      "patterns": [ "Bye", "See you later", "Goodbye" ],
      "responses": [ "See you later, thanks for visiting", "Have a nice day", "Bye! Come back again soon." ]
    },

    {
      "tag": "thanks",
      "patterns": [ "Thanks", "Thank you", "That's helpful" ],
      "responses": [ "Happy to help!", "Any time!", "My pleasure" ]
    },

    {
      "tag": "hours",
      "patterns": [ "What hours are you open?", "What are your hours?", "When are you open?" ],
      "responses": [ "We're open every day 9am-9pm", "Our hours are 9am-9pm every day" ]
    },

    {
      "tag": "shops",
      "patterns": [ "Which shops do you have delivery?", "What kinds of delivery mode?" ],
      "responses": [ "We have Cluck delivery." ]
    },

    {
      "tag": "payments",
      "patterns": [ "Do you take credit cards?", "Do you accept Mastercard?", "Are you cash only?", "Do you have GCASH?", "Can I pay load?" ],
      "responses": [ "We accept most major credit cards, GCash and Load" ]
    },

    {
      "tag": "reservation",
      "patterns": [ "Can I reserve?", "Which shop can we reserve", "How much is the reservation fee?" ],
      "responses": [ "We reserve for selected shops only." ]
    },

    {
      "tag": "delivery",
      "patterns": [ "Cluck delivery", "I'd like to have delivery from Cluck" ],
      "responses": [ "Are you looking for today delivery?" ],
      "context_set": "delivery_datetime"
    },

    {
      "tag": "today",
      "patterns": [ "today" ],
      "responses": [ "For delivery please call the shop." ],
      "context_filter": "delivery_datetime"
    },

    {
      "tag": "farm_register",
      "patterns": [ "farm register" ],
      "responses": [ "Would you like to request access for farm?" ]
    }
  ]
}